<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.GUI.controller.MainMenuController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="multiBtn" alignment="CENTER" layoutX="254.0" layoutY="191.0" mnemonicParsing="false" onAction="#playMultiplayer" prefHeight="55.0" prefWidth="92.0" text="Multiplayer" />
      <Button fx:id="singleBtn" layoutX="254.0" layoutY="253.0" mnemonicParsing="false" onAction="#playSinglePlayer" prefHeight="55.0" prefWidth="92.0" text="SinglePlayer" />
      <Button fx:id="quitBtn" layoutX="254.0" layoutY="315.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="55.0" prefWidth="92.0" text="Quit" />
   </children>
</AnchorPane>
